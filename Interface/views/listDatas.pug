extends layout

block content
  div(class='w3-container w3-margin-top')
    h1(class='w3-center w3-text-indigo') Indíce de Datas
    a(href='/', class='w3-button w3-indigo w3-margin-bottom') Voltar Atrás
    // Compact List with Improved Readability
    ul(class='w3-ul w3-hoverable') 
      each data in datas
        li
          a(href=`/search/data/${data}`, class='w3-text-indigo') #{data}
